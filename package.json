{
  "name": "reactica",
  "version": "0.0.6",
  "license": "MIT",
  "bin": {
    "reactica": "bin.js"
  },
  "files": [
    "dist",
    "link",
    "router",
    "auth",
    "client",
    "bin.js",
    "server-entry.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup && node build.mjs && cp src/vite/index.html dist/vite/plugin/index.html",
    "buildx": "yarn build:esm && yarn build:cjs",
    "tsup": "tsup && cp src/vite/index.html dist/vite/index.html",
    "dev": "tsup --watch",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./node": "./dist/vite/index.js",
    "./vite": "./dist/vite/plugin/index.js",
    "./cli": "./dist/cli/index.js",
    "./pages": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./cookies": {
      "require": "./dist/cookies.js",
      "import": "./dist/cookies.mjs",
      "types": "./dist/cookies.d.ts"
    },
    "./auth": {
      "require": "./dist/auth.js",
      "import": "./dist/auth.mjs",
      "types": "./dist/auth.d.ts"
    },
    "./head": {
      "require": "./dist/head.js",
      "import": "./dist/head.mjs",
      "types": "./dist/head.d.ts"
    },
    "./document": {
      "require": "./dist/document.js",
      "import": "./dist/document.mjs",
      "types": "./dist/document.d.ts"
    },
    "./routes": {
      "require": "./dist/routes.js",
      "import": "./dist/routes.mjs",
      "types": "./dist/routes.d.ts"
    },
    "./server": {
      "require": "./dist/server.js",
      "import": "./dist/server.mjs",
      "types": "./dist/server.d.ts"
    },
    "./client": {
      "require": "./dist/client.js",
      "import": "./dist/client.mjs",
      "types": "./dist/client.d.ts"
    },
    "./link": {
      "require": "./dist/link.js",
      "import": "./dist/link.mjs",
      "types": "./dist/link.d.ts"
    },
    "./router": {
      "require": "./dist/router.js",
      "import": "./dist/router.mjs",
      "types": "./dist/router.d.ts"
    },
    "./placeholder-loader": "./dist/placeholder-loader.mjs",
    "./language-redirect-client": "./dist/language-redirect-client.mjs"
  },
  "dependencies": {
    "@loadable/server": "^5.15.2",
    "debug": "^4.3.3",
    "deep-equal": "^2.0.5",
    "fast-glob": "^3.2.11",
    "json5": "^2.2.0",
    "local-pkg": "^0.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "react-router-dom": "^6.2.1",
    "yaml": "^2.0.0-10"
  },
  "devDependencies": {
    "@antfu/utils": "^0.5.0",
    "@brillout/libassert": "^0.5.4",
    "@hannoeru/eslint-config": "^0.6.0",
    "@types/debug": "^4.1.7",
    "@types/deep-equal": "^1.0.1",
    "@types/glob-to-regexp": "^0.4.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.2.0",
    "@vitest/ui": "^0.4.1",
    "@vue/compiler-sfc": "^3.2.31",
    "bumpp": "^7.1.1",
    "cross-env": "^7.0.3",
    "devalue": "^2.0.1",
    "esbuild": "^0.14.23",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.9.0",
    "esno": "^0.14.1",
    "html-entities": "^2.3.2",
    "node-fetch": "^2.6.6",
    "open": "^8.4.0",
    "playwright": "^1.19.1",
    "react-ssr-prepass": "^1.5.0",
    "solid-app-router": "^0.3.0",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5",
    "universal-cookie": "^4.0.4",
    "vite": "^2.8.4",
    "vite-plugin-inspect": "^0.3.14",
    "vite-plugin-pages": "^0.20.2",
    "vite-plugin-virtual": "^0.1.1",
    "vitest": "^0.4.1",
    "vue-router": "^4.0.12"
  }
}
