{
  "name": "reactica",
  "version": "0.0.6",
  "license": "MIT",
  "bin": {
    "reactica": "bin.js"
  },
  "files": [
    "dist",
    "link",
    "router",
    "auth",
    "client",
    "bin.js",
    "server-entry.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup && node build.mjs && cp src/vite/index.html dist/vite/index.html",
    "tsup": "tsup && cp src/vite/index.html dist/vite/index.html",
    "dev": "tsup --watch",
    "buildx": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./vite": "./dist/vite/index.js",
    "./cli": "./dist/cli/index.js",
    "./pages": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./server": "./dist/server.mjs",
    "./client": {
      "require": "./dist/client.js",
      "import": "./dist/client.mjs",
      "types": "./dist/client.d.ts"
    },
    "./placeholder-loader": "./dist/placeholder-loader.mjs",
    "./language-redirect-client": "./dist/language-redirect-client.mjs"
  },
  "dependencies": {
    "debug": "^4.3.3",
    "deep-equal": "^2.0.5",
    "fast-glob": "^3.2.11",
    "json5": "^2.2.0",
    "local-pkg": "^0.4.1",
    "yaml": "^2.0.0-10"
  },
  "devDependencies": {
    "@antfu/utils": "^0.5.0",
    "@hannoeru/eslint-config": "^0.6.0",
    "@types/debug": "^4.1.7",
    "@types/deep-equal": "^1.0.1",
    "@types/glob-to-regexp": "^0.4.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.18",
    "@vitest/ui": "^0.4.1",
    "@vue/compiler-sfc": "^3.2.31",
    "bumpp": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "esno": "^0.14.1",
    "playwright": "^1.19.1",
    "react-router": "^6.2.1",
    "solid-app-router": "^0.3.0",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vite-plugin-inspect": "^0.3.14",
    "vite-plugin-pages": "^0.20.2",
    "vitest": "^0.4.1",
    "vue-router": "^4.0.12"
  }
}
